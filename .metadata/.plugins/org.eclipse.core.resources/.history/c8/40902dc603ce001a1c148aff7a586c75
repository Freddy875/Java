
public class Variables {

	public static void main(String[] args){
		
	
		int MiNumero = 5;
		
		float MiNumeroFlotante = 5.99f;
		
		char MiLetra = 'D';
		
		boolean MiBooleano = true;
		
		String MiTexto = "Hola";
		
		String PrimerNombre = "John ";
		
		String Apellido = "Doe";
		
		
		String NombreCompleto = PrimerNombre + Apellido;
		
		System.out.println(MiNumero);
		
		System.out.println(MiNumeroFlotante);
		
		System.out.println(MiLetra);
		
		System.out.println(MiBooleano);
		
		System.out.println(MiTexto);
		
		System.out.println(NombreCompleto);
		
		int iNumeroX = 5, iNumeroY = 6;
		
		System.out.println(iNumeroX + iNumeroY);
		
		byte MiNumero2 = 100;
		
		System.out.println(MiNumero2);
		
		short MiCorto = 5000;
		
		System.out.println(MiCorto);
		
		long MiLargo = 15000000000L;
		
		System.out.println(MiLargo);
		
		double MiDoble = 19.99;
		
		System.out.println(MiDoble);
		
	}//fin public static void main

}//fin public class Variables
